<section id="send_validating_dialog">
  <header>
    <h1><%= _.str.sprintf(t('wallet.send.common.send_bitcoins'), ledger.config.network.plural()) %></h1>
  </header>
  <div id="content_container">
    <div class="regular-text"><%= t 'wallet.send.validating.validating_transaction' %></div>
    <div class="progress_container">
      <div id="progressbar_container"></div>
      <div class="regular-grey-text-small" id="progress_label">0%</div>
    </div>
    <div class="regular-grey-text-small"><%= t 'common.may_take_a_while' %></div>
  </div>
</section>
<div class="dialog-actions-bar">
  <div class="left-spacer"></div>
  <a class="cancel-rounded-button" href="#cancel"><%= t 'common.cancel' %></a>
</div>